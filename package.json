{
  "name": "vue-pmndrs",
  "icon": "assets/vue-pmndrs.png",
  "displayName": "vue-poimandres",
  "description": "improved vue syntax styling for the poimandres theme",
  "version": "0.0.1",
  "keywords": [
    "vue-poimandres",
    "vue-pmndrs"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "dev": "node src/watcher"
  },
  "author": "Paul Henschel & Brandon Saldan",
  "publisher": "BrandonSaldan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonsaldan/poimandres-vue/issues"
  },
  "homepage": "https://github.com/brandonsaldan/poimandres-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonsaldan/poimandres-vue.git"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "vue-poimandres",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme.json"
      },
      {
        "label": "vue-poimandres-noitalics",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-noitalics.json"
      },
      {
        "label": "vue-poimandres-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-storm.json"
      },
      {
        "label": "vue-poimandres-noitalics-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/poimandres-color-theme-noitalics-storm.json"
      }
    ]
  },
  "devDependencies": {
    "filewatcher": "^3.0.1",
    "prettier": "^2.2.1",
    "sharp": "^0.28.0"
